{
	"env": {
		"browser": true
	},
	"root": true,
	"extends": "eslint:recommended",
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
		],
		"no-console": 0,
		"no-unused-vars": ["error", { "vars": "all", "args": "after-used" }]
	}
}